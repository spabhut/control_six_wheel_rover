<mujoco model="six_wheel_rover">
  <compiler angle="radian" />
  
  <option gravity="0 0 -9.81" timestep="0.001"/>
  
  <visual>
    <headlight ambient="0.5 0.5 0.5" />
  </visual>
  
  <asset>
    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>
  
  <worldbody>
    <!-- Ground plane -->
    <geom name="floor" type="plane" size="10 10 0.1" material="grid" condim="3" friction="1 0.005 0.0001"/>
    
    <light pos="0 0 3" dir="0 0 -1" diffuse="1 1 1"/>
    
    <!-- Rover Base -->
    <body name="base_link" pos="0 0 0.15">
      <freejoint/>
      
      <!-- Chassis -->
      <geom name="chassis" type="box" size="0.4 0.2 0.1" rgba="0.7 0.7 0.7 1" mass="5"/>
      
      <!-- Inertial properties -->
      <inertial pos="0 0 0" mass="5" diaginertia="0.1 0.1 0.1"/>
      
      <!-- Front Left Wheel -->
      <body name="front_left_wheel" pos="0.25 0.225 -0.05">
        <joint name="front_left_joint" type="hinge" axis="0 1 0" damping="0.1" armature="0.02"/>
        <geom name="fl_wheel" type="cylinder" size="0.1 0.025" rgba="0.2 0.2 0.2 1" mass="0.5" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
        <inertial pos="0 0 0" mass="0.5" diaginertia="0.005 0.005 0.005"/>
      </body>
      
      <!-- Front Right Wheel -->
      <body name="front_right_wheel" pos="0.25 -0.225 -0.05">
        <joint name="front_right_joint" type="hinge" axis="0 1 0" damping="0.1" armature="0.02"/>
        <geom name="fr_wheel" type="cylinder" size="0.1 0.025" rgba="0.2 0.2 0.2 1" mass="0.5" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
        <inertial pos="0 0 0" mass="0.5" diaginertia="0.005 0.005 0.005"/>
      </body>

      <!-- Middle Left Wheel -->
      <body name="middle_left_wheel" pos="0.0 0.225 -0.05">
        <joint name="middle_left_joint" type="hinge" axis="0 1 0" damping="0.1" armature="0.02"/>
        <geom name="ml_wheel" type="cylinder" size="0.1 0.025" rgba="0.2 0.2 0.2 1" mass="0.5" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
        <inertial pos="0 0 0" mass="0.5" diaginertia="0.005 0.005 0.005"/>
      </body>
      
      <!-- Middle Right Wheel -->
      <body name="middle_right_wheel" pos="0.0 -0.225 -0.05">
        <joint name="middle_right_joint" type="hinge" axis="0 1 0" damping="0.1" armature="0.02"/>
        <geom name="mr_wheel" type="cylinder" size="0.1 0.025" rgba="0.2 0.2 0.2 1" mass="0.5" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
        <inertial pos="0 0 0" mass="0.5" diaginertia="0.005 0.005 0.005"/>
      </body>
      
      <!-- Rear Left Wheel -->
      <body name="rear_left_wheel" pos="-0.25 0.225 -0.05">
        <joint name="rear_left_joint" type="hinge" axis="0 1 0" damping="0.1" armature="0.02"/>
        <geom name="rl_wheel" type="cylinder" size="0.1 0.025" rgba="0.2 0.2 0.2 1" mass="0.5" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
        <inertial pos="0 0 0" mass="0.5" diaginertia="0.005 0.005 0.005"/>
      </body>
      
      <!-- Rear Right Wheel -->
      <body name="rear_right_wheel" pos="-0.25 -0.225 -0.05">
        <joint name="rear_right_joint" type="hinge" axis="0 1 0" damping="0.1" armature="0.02"/>
        <geom name="rr_wheel" type="cylinder" size="0.1 0.025" rgba="0.2 0.2 0.2 1" mass="0.5" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
        <inertial pos="0 0 0" mass="0.5" diaginertia="0.005 0.005 0.005"/>
      </body>
    </body>
  </worldbody>
  
  <actuator>
    <!-- Velocity actuators for each wheel -->
    <velocity name="fl_motor" joint="front_left_joint" kv="10"/>
    <velocity name="fr_motor" joint="front_right_joint" kv="10"/>
    <velocity name="ml_motor" joint="middle_left_joint" kv="10"/>
    <velocity name="mr_motor" joint="middle_right_joint" kv="10"/>
    <velocity name="rl_motor" joint="rear_left_joint" kv="10"/>
    <velocity name="rr_motor" joint="rear_right_joint" kv="10"/>
  </actuator>
</mujoco>