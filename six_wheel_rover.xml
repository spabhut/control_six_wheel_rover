<mujoco model="six_wheel_rover">
  <compiler angle="radian" />
  
  <option gravity="0 0 -9.81" timestep="0.0005"/>
  
  <visual>
    <headlight ambient="0.5 0.5 0.5" />
  </visual>
  
  <asset>
    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>
  
  <worldbody>
    <geom name="floor" type="plane" size="30 30 0.1" material="grid" condim="3" friction="1 0.005 0.0001"/>
    <light pos="0 0 3" dir="0 0 -1" diffuse="1 1 1"/>
    
    <body name="base_link" pos="0 0 0.2">
      <freejoint/>
      
      <geom name="chassis" type="box" size="0.5 0.25 0.1" pos="0 0 0.15" rgba="0.7 0.7 0.7 1" mass="10"/>
      
      <inertial pos="0 0 0.15" mass="10" diaginertia="0.5 0.5 0.01"/>
      
      <body name="left_front_wheel" pos="0.4 0.275 0.1">
        <joint name="left_front_joint" type="hinge" axis="0 1 0" damping="0.1"/>
        <inertial pos="0 0 0" mass="1.0" diaginertia="0.01 0.01 0.01"/>
        <geom name="lf_geom" type="cylinder" size="0.1 0.025" rgba="0.1 0.1 0.1 1" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
      </body>

      <body name="left_middle_wheel" pos="0.0 0.275 0.1">
        <joint name="left_middle_joint" type="hinge" axis="0 1 0" damping="0.1"/>
        <inertial pos="0 0 0" mass="1.0" diaginertia="0.01 0.01 0.01"/>
        <geom name="lm_geom" type="cylinder" size="0.1 0.025" rgba="0.1 0.1 0.1 1" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
      </body>

      <body name="left_rear_wheel" pos="-0.4 0.275 0.1">
        <joint name="left_rear_joint" type="hinge" axis="0 1 0" damping="0.1"/>
        <inertial pos="0 0 0" mass="1.0" diaginertia="0.01 0.01 0.01"/>
        <geom name="lr_geom" type="cylinder" size="0.1 0.025" rgba="0.1 0.1 0.1 1" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
      </body>

      <body name="right_front_wheel" pos="0.4 -0.275 0.1">
        <joint name="right_front_joint" type="hinge" axis="0 1 0" damping="0.1"/>
        <inertial pos="0 0 0" mass="1.0" diaginertia="0.01 0.01 0.01"/>
        <geom name="rf_geom" type="cylinder" size="0.1 0.025" rgba="0.1 0.1 0.1 1" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
      </body>

      <body name="right_middle_wheel" pos="0.0 -0.275 0.1">
        <joint name="right_middle_joint" type="hinge" axis="0 1 0" damping="0.1"/>
        <inertial pos="0 0 0" mass="1.0" diaginertia="0.01 0.01 0.01"/>
        <geom name="rm_geom" type="cylinder" size="0.1 0.025" rgba="0.1 0.1 0.1 1" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
      </body>

      <body name="right_rear_wheel" pos="-0.4 -0.275 0.1">
        <joint name="right_rear_joint" type="hinge" axis="0 1 0" damping="0.1"/>
        <inertial pos="0 0 0" mass="1.0" diaginertia="0.01 0.01 0.01"/>
        <geom name="rr_geom" type="cylinder" size="0.1 0.025" rgba="0.1 0.1 0.1 1" friction="1 0.005 0.0001" euler="1.5708 0 0"/>
      </body>

    </body>
  </worldbody>
  
  <actuator>
    <velocity name="lf_motor" joint="left_front_joint" kv="1"/>
    <velocity name="lm_motor" joint="left_middle_joint" kv="1"/>
    <velocity name="lr_motor" joint="left_rear_joint" kv="1"/>
    
    <velocity name="rf_motor" joint="right_front_joint" kv="1"/>
    <velocity name="rm_motor" joint="right_middle_joint" kv="1"/>
    <velocity name="rr_motor" joint="right_rear_joint" kv="1"/>
  </actuator>
</mujoco>